@model IEnumerable<WebBanPhuKienDienThoai.Models.DeviceType>

<div class="container mt-4">
    <h2 class="mb-3">Danh sách thiết bị</h2>
    <table class="table table-striped table-hover shadow-sm">
        <thead class="table-dark">
            <tr>
                <th>ID</th>
                <th>Tên</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var deviceType in Model)
            {
                <tr>
                    <td>@deviceType.Id</td>
                    <td>@deviceType.Name</td>
                 
                </tr>
            }
        </tbody>
    </table>
</div>